setwd("~/Downloads/_Machine Learning/Machine Learning A-Z Template Folder/Part 4 - Clustering/Section 24 - K-Means Clustering")
dataset <- read.csv('Mall_Customers.csv')
X <- dataset[4:5]
View(X)
set.seed(6)
wcss <- vector()
for (i in 1:10) wcss[i] <- sum (kmeans(X, i)$withinss)
plot(1:10, wcss, type = 'b', main = paste('Clusters of Customers'), xlab = "Number of Clusters", ylab = 'WCSS')
set.seed(29)
kmeans <- kmeans(X, 5, iter.max = 300, nstart = 10)
View(kmeans)
library(cluster)
clusplot(X,
kmeans$cluster,
lines = 0,
shade = TRUE,
color = TRUE,
labels = 2,
plotchar = FALSE,
span = TRUE,
main = paste('Clusters of Customers'),
xlab = "Annual Income",
ylab = 'Spending Score')
